<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="editText">
        <item name="android:fontFamily">@font/sigmar</item>
        <item name="colorPrimary">@color/primary_dark</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

        <style name="CircleShape">
            <item name="cornerFamily">rounded</item>
            <item name="cornerSize">50%</item>
        </style>

    <style name="CalendarDarkTheme" parent="Theme.MaterialComponents.DayNight">
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="android:textColor">@color/primary_dark</item>
        <item name="android:dateTextAppearance">@style/CalendarDateTextAppearance</item>
    </style>

    <style name="CalendarDateTextAppearance" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">@color/primary_dark</item>
    </style>



</resources>